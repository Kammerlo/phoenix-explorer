{
  "version": 2,
  "builds": [
    {
      "src": "packages/backend/api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "packages/frontend/vite.config.ts",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "packages/backend/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "packages/frontend/index.html"
    }
  ]
}
