{
  "version": 2,
  "builds": [
    {
      "src": "packages/backend/src/app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "packages/backend/src/app.ts"
    },
    {
      "src": "/(.*)",
      "dest": "packages/frontend/dist/$1"
    }
  ],
  "outputDirectory": "packages/frontend/dist"
}
